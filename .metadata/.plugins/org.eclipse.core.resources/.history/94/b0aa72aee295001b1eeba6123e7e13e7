package com.xworkz.component;

public class Power {
	boolean on;

	public Power() {

		System.out.println("obj created for:" + this.getClass().getSimpleName());
	}

	public boolean isOn() {
		System.out.println("invoked isOn method");
		if (on) {
			return true;
		} else {
			return false;
		}
	}

	public void setOn(boolean on) {
		System.out.println("invoked setOn method");
		this.on = on;

	}

}
